/bin/bash -c "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)"
/opt/homebrew/bin/brew install --cask google-chrome
/opt/homebrew/bin/brew install --cask intellij-idea-ce
